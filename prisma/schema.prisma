// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

model manchete {
  id            String  @id @default(cuid())
  title         String 
  description   String
  link          String?
  expiration    DateTime?
  @@map("manchetes")
}